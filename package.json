  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint --fix \"src/**/*.ts\"",
    "conc:example": "concurrently \"npm run build:server\" \"nodemon ./server/dist/server.js\" ",
    "start": "npm run build:client && npm run server",
    "server": "tsc-watch -p ./server --onSuccess \"node ./server/dist/server.js\" ",
    "build:server": "tsc -p ./server",
    "build:server:watch": "tsc -w -p ./server",
    "build:client": "ng build",
    "run:client": "ng serve",
    "client": "npm run build:client && npm run run:client"
  },
