{
	"extends": ["tslint-angular"],
	"rules":
	 {
		"prefer-const": false,
		"indent": [
			true,
			"tabs"
		],
		"no-use-before-declare":false,
		"deprecation": false,

	  "directive-selector": [false, "attribute", "foo", "camelCase"],
	  "component-selector": [false, "element", "foo", "kebab-case"]
	}
  }
